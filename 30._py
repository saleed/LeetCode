
# coding:utf-8
class Solution(object):
    def findSubstring(self, s, words):
        """
        :type s: str
        :type words: List[str]
        :rtype: List[int]
        """
        allcombine=[]
        res=[]
        self.dfs(words,[],allcombine)
        for comb in allcombine:
            tmp_res=self.MatchSubStr(s,comb)
            if len(tmp_res)==0:
                continue
            res.extend(tmp_res)

        res=list(set(res))
        res.sort()
        return res

    def dfs(self,words,tmp_res,res):
        if len(words)==0:
            res.append("".join(tmp_res[:]))
            return
        else:
            for val in words:
                tmp_res.append(val)
                newwords=words[:]
                newwords.remove(val)
                self.dfs(newwords,tmp_res,res)
                tmp_res.pop()

    def MatchSubStr(self,words,substr):
        if len(substr)>len(words):
            return []
        res=[]
        p=0
        while p+len(substr)-1<len(words):
            tmpsubstr = words[p:p+len(substr)]
            if tmpsubstr==substr:
                res.append(p)
            p+=1
        return res

if __name__=="__main__":
    a=Solution()
    test=""
    words=["dhvf", "sind", "ffsl", "yekr", "zwzq", "kpeo", "cila", "tfty", "modg", "ztjg", "ybty", "heqg", "cpwo", "gdcj","lnle", "sefg", "vimw", "bxcb"]
    res=a.findSubstring(test,words)
    print(res)





